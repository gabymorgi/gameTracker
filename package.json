{
  "name": "game-tracker",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "netlify dev",
    "dev": "vite",
    "build": "tsc && vite build",
    "postinstall": "npx prisma generate",
    "script": "cross-env TS_NODE_PROJECT='./scripts/tsconfig.json' node --loader ts-node/esm ./scripts/main.ts",
    "lint": "npm run lint:types && npm run lint:code",
